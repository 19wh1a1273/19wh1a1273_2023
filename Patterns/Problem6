INPUT: 5
OUTPUT:
* * * * * 
 * * * * 
  * * * 
   * * 
    * 
    
Program:

#include <bits/stdc++.h>
using namespace std;
int main() {
   int i,j,n,s,coef=1;
   cin>>n;
   for(i=n;i>0;i--){
       for(s=0;s<n-i;s++){
           cout<<" ";
       }
       for(j=i;j>0;j--){
           cout<<"* ";
       }
       cout<<endl;
   }
   return 0;
}
